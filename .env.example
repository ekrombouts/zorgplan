# OpenAI Configuration
# Set to true to use Azure OpenAI, false to use regular OpenAI
USE_AZURE_OPENAI=true

# Set to true to disable tracing (recommended for privacy)
DISABLE_TRACING=true

# Azure OpenAI Configuration (required when USE_AZURE_OPENAI=true)
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_API_VERSION=2024-02-15-preview
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_DEPLOYMENT=your_deployment_name

# Regular OpenAI Configuration (required when USE_AZURE_OPENAI=false)
OPENAI_API_KEY=your_openai_api_key_here

# Model Configuration
DEFAULT_MODEL=gpt-4o-mini
AZURE_MODEL=gpt-4o
